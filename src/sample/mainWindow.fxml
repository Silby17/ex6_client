<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="gpMain" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="620.0" prefWidth="414.0"
          xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/8"
          fx:controller="sample.mainController"
          styleClass="hbLogin">

    <columnConstraints>
        <ColumnConstraints/>

    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>

    <children>
        <HBox fx:id="hbMain" prefHeight="1000.0" prefWidth="1000.0"
              spacing="10.0">
            <children>

                <Button fx:id="btnConn" layoutX="50" layoutY="50"
                        mnemonicParsing="false" prefHeight="69.0"
                        prefWidth="130.0"
                        styleClass="btnWithImage"/>

                <ChoiceBox fx:id="cbOptions" prefWidth="150.0"/>

                <Button
                        fx:id="btnSearch" layoutX="50" layoutY="50"
                        mnemonicParsing="false" prefHeight="69.0"
                        prefWidth="130.0"
                        styleClass="btnSearch"/>

                <Button mnemonicParsing="false" text="Add"/>
                <Button mnemonicParsing="false" text="Delete"/>
                <Button mnemonicParsing="false" text="All"/>
            </children>
        </HBox>
    </children>
</GridPane>
